FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{

    U
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          0;
    }

    UFinal
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          0;
    }

    "p.*"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-8;
        relTol          0.0;
    }

/*
    "p.*"
    {
        solver           GAMG;
        tolerance        1e-8;
        //relTol           0.01;
        relTol           0.0;
        smoother         GaussSeidel;
        nPreSweeps       0;
        nPostSweeps      2;
        cacheAgglomeration on;
        agglomerator     faceAreaPair;
        //nCellsInCoarsestLevel 100;
        nCellsInCoarsestLevel 10;
        mergeLevels      1;
    }
*/

    "e.*"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-8;
        relTol          0.0;
    }

    "rho.*"
    {
        solver          diagonal;
    }

    "(k|epsilon|omega).*"
    {
        $p;
        tolerance       1e-8;
    }

}

PIMPLE
{
    momentumPredictor 	yes;
    consistent 		yes; 

    nOuterCorrectors     2;
    nCorrectors          2;
    nNonOrthogonalCorrectors 1;

    transonic 	yes;

