FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (680 0 0)
);

regions
(
    boxToCell
    {
        box (0 -100 -100) (100 100 100);
        fieldValues
        (
            volVectorFieldValue U (670 118 0)
        );
    }
);

