FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform ( 0.434495494 0.038013430 0 ) ;

boundaryField
{

	Side
    	{
        	type            empty;
    	}
   	
	Outlet
    	{
        	type            zeroGradient;
    	}
   	
	Airfoil
    	{
        	type            noSlip;
    	}
   	
	Inlet
    	{
        	type            inletOutlet;
		inletValue	uniform ( 0.434495494 0.038013430 0 );
    	}


}


