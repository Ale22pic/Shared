FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 3.384e-5;

boundaryField
{
   
    freestream
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    fuso
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    out
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    symmetry
    {
        type 		symmetry;
    }
    wing
    {
        type 		zeroGradient; //fixedValue;
        //value		uniform 0;
    }

}


