// Field Dictionary

FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          p;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1.;

boundaryField
{
    
    freestream
    {
        type 		freestreamPressure;
        freestreamValue uniform 1;
    }
    fuso
    {
        type 		zeroGradient;
    }
    out
    {
        type 		freestreamPressure;
        freestreamValue uniform 1;
    }
    symmetry
    {
        type 		symmetry;
    }
    wing
    {
        type 		zeroGradient;
    }

}


